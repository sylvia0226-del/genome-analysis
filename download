    $(function() {
        bizx.cmp.ifConsent({ purposes: 'all' , vendors: 'narrative'}, function() {
            var current_time = (new Date()).getTime();
            let imageUrl = "https://io.narrative.io/?companyId=2440&id=first_party%3A27b71a73-b8b0-48ad-937b-cb5bd7d7f51f&id=site_name%3Asourceforge.net&id=url%3A%2Fprojects%2Fmauve%2F&id=pagetitle%3AMauve+download+%7C+SourceForge.net&id=vertical%3AOpen+Source+Software&id=product%3AMauve&id=topics%3ABio-Informatics%2CData+Visualization%2CMedical&id=programming_language%3AC%2B%2B%2CJava";
            imageUrl = imageUrl.replace(encodeURIComponent("$PAGE_TITLE"), document.title);  
            
            imageUrl = URL.parse(imageUrl);
            let hem = bizx.uids.getHem();
            if (hem) {
                imageUrl.searchParams.append('id', 'hem:' + hem);
            }
            imageUrl.searchParams.append('rand', current_time);

            var image = new Image();
            image.src = imageUrl.toString();
            image.style.display = "none";
            image.style.height = 0;
            image.style.width = 0;
            document.body.appendChild(image);
        });
    })
</script>
        

        









